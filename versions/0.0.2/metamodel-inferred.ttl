@prefix : <http://emmo.info/datamodel-inferred> .
@prefix datamodel: <http://emmo.info/datamodel#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

datamodel:hasRelation a owl:DatatypeProperty ;
    rdfs:comment """In the case that the type of a property refer to an entity, 'hasRelation' can be used to specify the type of relation between the property and the referred entity. 

Typical values are:
  "dm:composition"
  "dm:connection"
  "dm:relation"
or any of their sub-properties."""@en ;
    rdfs:subPropertyOf datamodel:dataRelation ;
    skos:prefLabel "hasRelation"@en .

datamodel:hasType a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:comment "May change in the future."@en,
        "Refer to a comment. Normally just a string with a type name, like \"float\". Ideally this should be an object property referring to a type in an ontology."@en ;
    rdfs:domain datamodel:Property ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf datamodel:dataComposition ;
    skos:prefLabel "hasType"@en .

datamodel:instanceOf a owl:AnnotationProperty ;
    rdfs:comment "Relates an instance to the class that describes it."@en,
        """The semantic meaning of 'instanceOf' is the same as 'rdf:type'.

But since first order logic, of which OWL descriptive logic is based on, we introduce 'instanceOf' as an annotation property."""@en ;
    rdfs:domain datamodel:Instance ;
    rdfs:isDefinedBy <rdf:type> ;
    rdfs:range datamodel:Metadata ;
    skos:prefLabel "instanceOf"@en .

datamodel:relation a owl:ObjectProperty ;
    rdfs:comment """In UML this is is a subproperty of association that is not an aggregation.

In EMMO this may be interpreted as 'semiotical'."""@en,
        "Note that this object property is not in the datamodel ontology, but may be used as a value of the 'hasRelation' data property of 'Property'."@en ;
    rdfs:domain datamodel:DataModel ;
    rdfs:range datamodel:DataModel ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    skos:prefLabel "relation"@en .

: a owl:Ontology .

dcterms:abstract a owl:AnnotationProperty .

dcterms:contributor a owl:AnnotationProperty .

dcterms:creator a owl:AnnotationProperty .

dcterms:license a owl:AnnotationProperty .

dcterms:publisher a owl:AnnotationProperty .

dcterms:title a owl:AnnotationProperty .

skos:altLabel a owl:AnnotationProperty ;
    rdfs:subPropertyOf rdfs:label .

skos:prefLabel a owl:AnnotationProperty ;
    rdfs:subPropertyOf rdfs:label .

datamodel:DataInstance a owl:Class ;
    datamodel:instanceOf datamodel:Entity ;
    rdfs:comment """An instance representing actual data.

It is hence disjoint from Metadata."""@en ;
    rdfs:subClassOf datamodel:Instance ;
    skos:prefLabel "DataInstance"@en .

datamodel:EMMO_23057c59_2137_40ab_84a4_748933a689df a owl:ObjectProperty ;
    rdfs:comment "The shape of a property. May be an empty shape."@en ;
    rdfs:domain datamodel:Property ;
    rdfs:range datamodel:Shape ;
    rdfs:subPropertyOf datamodel:composition ;
    skos:prefLabel "hasShape"@en .

datamodel:EntitySchema a owl:Class ;
    datamodel:instanceOf datamodel:BasicMetadataSchema ;
    rdfs:comment "A meta-metadata that describes an entity."@en ;
    rdfs:subClassOf datamodel:Entity ;
    skos:prefLabel "EntitySchema"@en .

datamodel:aggregation a owl:ObjectProperty ;
    rdfs:comment "A connection between two entities."@en,
        """In UML this is is a subproperty of aggregation that is not a composition.

In EMMO this may be interpreted as 'topological'."""@en ;
    rdfs:domain datamodel:DataModel ;
    rdfs:range datamodel:DataModel ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    skos:prefLabel "connection"@en .

datamodel:dataRelation a owl:DatatypeProperty ;
    rdfs:comment "A generic relation between a data attribute and a DataModel subclass that is not a connection or composition."@en,
        """In UML this is is a subproperty of association that is not an aggregation.

In EMMO this may be interpreted as 'semiotical'."""@en ;
    rdfs:subPropertyOf owl:topDataProperty ;
    skos:prefLabel "dataRelation"@en .

datamodel:hasDimensionSize a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:comment "The size of a dimension value."@en ;
    rdfs:domain datamodel:DimensionValue ;
    rdfs:range xsd:integer ;
    rdfs:subPropertyOf owl:topDataProperty ;
    skos:prefLabel "hasDimensionSize"@en .

datamodel:hasDimensionValue a owl:ObjectProperty ;
    rdfs:domain datamodel:Instance ;
    rdfs:range datamodel:DimensionValue ;
    rdfs:subPropertyOf datamodel:composition ;
    skos:prefLabel "hasDimensionValue"@en .

datamodel:hasFirst a owl:ObjectProperty ;
    rdfs:comment "Relates a whole with an ordered set of parts (elements) to its first element."@en ;
    rdfs:subPropertyOf datamodel:composition ;
    skos:prefLabel "hasFirst"@en .

datamodel:hasFirstDimensionExpression a owl:ObjectProperty ;
    rdfs:comment "The first dimension expression of a shape."@en ;
    rdfs:domain datamodel:Shape ;
    rdfs:range datamodel:DimensionExpression ;
    rdfs:subPropertyOf datamodel:hasFirst ;
    skos:prefLabel "hasFirstDimensionExpression"@en .

datamodel:hasMeta a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:comment "Relates an instance to the URI that uniquely identifies the metadata that describes the instance."@en ;
    rdfs:domain datamodel:Instance ;
    rdfs:range xsd:anyURI ;
    rdfs:subPropertyOf datamodel:dataComposition ;
    skos:prefLabel "hasMeta"@en .

datamodel:hasNext a owl:ObjectProperty ;
    rdfs:comment "Relates an element in an ordered list to its next element."@en ;
    rdfs:subPropertyOf datamodel:aggregation ;
    skos:prefLabel "hasNext"@en .

datamodel:hasNextDimensionExpression a owl:AsymmetricProperty,
        owl:FunctionalProperty,
        owl:InverseFunctionalProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:comment "Relates a dimension expression to the next dimension expression."@en ;
    rdfs:domain datamodel:DimensionExpression ;
    rdfs:range datamodel:DimensionExpression ;
    rdfs:subPropertyOf datamodel:hasNext ;
    skos:prefLabel "hasNextDimensionExpression"@en .

datamodel:hasPropertyValue a owl:ObjectProperty ;
    rdfs:domain datamodel:Instance ;
    rdfs:range datamodel:PropertyValue ;
    rdfs:subPropertyOf datamodel:composition ;
    skos:prefLabel "hasPropertyValue"@en .

datamodel:hasUUID a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:comment "Refers to a unique UUID identifying an instance."@en ;
    rdfs:domain datamodel:Instance ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf owl:topDataProperty ;
    skos:prefLabel "hasUUID"@en .

datamodel:hasUnit a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:comment "May be omitted for dimensionless quantities."@en,
        "May change in the future."@en,
        "Refer to a unit. Normally just a string with a conventional unit, like \"m/s\". Ideally this should be an object property referring to a unit in an ontology."@en ;
    rdfs:domain datamodel:Property ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf datamodel:dataComposition ;
    skos:prefLabel "hasUnit"@en .

datamodel:hasValue a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:comment """The actual literal value of a dimension expression or a property of a data object.

The syntax of the actual value is implementation dependent."""@en ;
    rdfs:domain datamodel:DataModel ;
    rdfs:range rdf:PlainLiteral ;
    rdfs:subPropertyOf datamodel:dataComposition ;
    skos:prefLabel "hasValue"@en .

datamodel:BasicMetadataSchema a owl:Class ;
    datamodel:instanceOf datamodel:BasicMetadataSchema ;
    rdfs:comment "A basic metadata schema that can describe itself."@en ;
    rdfs:subClassOf datamodel:Entity ;
    skos:prefLabel "BasicMetadataSchema"@en .

datamodel:PropertyValue a owl:Class ;
    rdfs:comment "The value of a property of a data object. Since the type system is kept open-ended there are no restriction on the actual value in this ontology. In an actual implementation there will be a restriction."@en ;
    rdfs:subClassOf datamodel:DataModel ;
    skos:prefLabel "PropertyValue"@en .

datamodel:hasLabel a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:comment "Range is defined as xsd:string, even though xsd:NCName would have been more precise, but that is not supported by the FaCT++ reasoner."@en,
        "Refer to a unique and valid C identifier."@en ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf datamodel:dataComposition ;
    skos:prefLabel "hasLabel"@en .

datamodel:hasProperty a owl:ObjectProperty ;
    rdfs:comment "Relates an entity to one of its properties."@en ;
    rdfs:domain datamodel:Entity ;
    rdfs:range datamodel:Property ;
    rdfs:subPropertyOf datamodel:composition ;
    skos:prefLabel "hasProperty"@en .

datamodel:DimensionValue a owl:Class ;
    rdfs:comment "Value of a dimension of a an instance."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:integer ;
            owl:onProperty datamodel:hasDimensionSize ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        datamodel:DataModel ;
    skos:prefLabel "DimensionValue"@en .

datamodel:Shape a owl:Class ;
    rdfs:comment "An element in the list of of dimension in a property."@en,
        "ShapeElement would have been a better name."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass datamodel:DimensionExpression ;
            owl:onProperty datamodel:hasFirstDimensionExpression ],
        datamodel:DataModel ;
    skos:prefLabel "Shape"@en .

datamodel:hasDimension a owl:ObjectProperty ;
    rdfs:comment "Relates an entity to one of its dimensions."@en ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( datamodel:DimensionExpression datamodel:Entity ) ] ;
    rdfs:range datamodel:Dimension ;
    rdfs:subPropertyOf datamodel:composition ;
    skos:prefLabel "hasDimension"@en .

datamodel:hasIdentity a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:comment "Unique identity of an entity. This will typically be its IRI."@en ;
    rdfs:domain datamodel:DataModel ;
    rdfs:range xsd:anyURI ;
    rdfs:subPropertyOf datamodel:dataComposition ;
    skos:altLabel "hasURI"@en ;
    skos:prefLabel "hasIdentity"@en .

datamodel:Dimension a owl:Class ;
    rdfs:comment "A dimension of an Entity. Dimensions have a label, which i unique within the scope of the Entity and a human description."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty datamodel:hasLabel ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty datamodel:hasDescription ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        datamodel:DataModel ;
    skos:prefLabel "Dimension"@en .

datamodel:hasDescription a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:comment "An human readable description of a class."@en ;
    rdfs:domain datamodel:DataModel ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf datamodel:dataComposition ;
    skos:prefLabel "hasDescription"@en .

owl:topObjectProperty a owl:ReflexiveProperty,
        owl:SymmetricProperty,
        owl:TransitiveProperty ;
    owl:inverseOf owl:topObjectProperty .

datamodel:Entity a owl:Class ;
    datamodel:instanceOf datamodel:EntitySchema ;
    rdfs:comment "Metadata describing a data object."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty datamodel:hasDimension ;
            owl:someValuesFrom datamodel:Dimension ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:anyURI ;
            owl:onProperty datamodel:hasIdentity ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty datamodel:hasDescription ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onProperty datamodel:hasProperty ;
            owl:someValuesFrom datamodel:Property ],
        datamodel:Metadata ;
    owl:equivalentClass datamodel:Metadata ;
    skos:altLabel "Metadata"@en ;
    skos:prefLabel "Entity"@en .

datamodel:Metadata a owl:Class ;
    rdfs:comment "Metadata describing its instances."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty datamodel:hasProperty ;
            owl:someValuesFrom datamodel:Property ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty datamodel:hasDescription ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:anyURI ;
            owl:onProperty datamodel:hasIdentity ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onProperty datamodel:hasDimension ;
            owl:someValuesFrom datamodel:Dimension ],
        datamodel:Instance ;
    skos:prefLabel "Metadata"@en .

datamodel:DimensionExpression a owl:Class ;
    rdfs:comment "A dimension expression is a mathematical expression of dimension labels."@en,
        """Examples:
"N"
"N+1"
"N+2*M\""""@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty datamodel:hasDimension ;
            owl:someValuesFrom datamodel:Dimension ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty datamodel:hasValue ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass datamodel:DimensionExpression ;
            owl:onProperty datamodel:hasNextDimensionExpression ],
        datamodel:DataModel ;
    skos:prefLabel "DimensionExpression"@en .

datamodel:Property a owl:Class ;
    rdfs:comment """A property describes one real data element in the corresponding object (instance) in terms of its
  - type
  - shape
  - unit
In addition have properties a label (unique within the scope of the entity) and a human description."""@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty datamodel:hasLabel ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty datamodel:hasDescription ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass datamodel:Shape ;
            owl:onProperty datamodel:EMMO_23057c59_2137_40ab_84a4_748933a689df ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty datamodel:hasUnit ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        datamodel:DataModel ;
    skos:prefLabel "Property"@en .

datamodel:composition a owl:ObjectProperty ;
    rdfs:comment """In UML this corresponds to UML composition.

In EMMO this is a 'mereological'."""@en,
        "Relates a part to its whole."@en ;
    rdfs:domain datamodel:DataModel ;
    rdfs:range datamodel:DataModel ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    skos:altLabel "hasPart"@en ;
    skos:prefLabel "composition"@en .

datamodel:Instance a owl:Class ;
    datamodel:instanceOf datamodel:Metadata ;
    rdfs:comment """An instance of a metadata.

Instances are uniquely identified by their UUID, but may also have an more human readable identity.

Instances holds the values of the dimensions and the properties described by their metadata."""@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty datamodel:hasDimensionValue ;
            owl:someValuesFrom datamodel:DimensionValue ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:anyURI ;
            owl:onProperty datamodel:hasIdentity ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:anyURI ;
            owl:onProperty datamodel:hasMeta ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty datamodel:hasUUID ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onProperty datamodel:hasPropertyValue ;
            owl:someValuesFrom datamodel:PropertyValue ],
        datamodel:DataModel ;
    owl:disjointUnionOf ( datamodel:DataInstance datamodel:Metadata ) ;
    skos:altLabel "DataObject"@en ;
    skos:prefLabel "Instance"@en .

datamodel:dataComposition a owl:DatatypeProperty ;
    rdfs:comment "Relates a data attribute to the whole (DataModel subclass) that it is a part of."@en ;
    rdfs:domain datamodel:DataModel ;
    rdfs:subPropertyOf owl:topDataProperty ;
    skos:prefLabel "dataComposition"@en .

datamodel:DataModel a owl:Class ;
    rdfs:comment "A superclass for all classes in the data model ontology."@en ;
    rdfs:subClassOf owl:Thing ;
    skos:prefLabel "DataModel"@en .


            owl:unionOf ( emmo:EMMO_669d2749_bece_460a_b26a_9a909fd8ca4d emmo:EMMO_881606d0_6f2f_4947_bc8b_75c5b7b2b688 emmo:EMMO_c9805ac9_a943_4be4_ac4b_6da64ba36c73 ) ] ;

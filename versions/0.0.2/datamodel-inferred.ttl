            owl:unionOf ( emmo:EMMO_0cd58641_824c_4851_907f_f4c3be76630c emmo:EMMO_35d2e130_6e01_41ed_94f7_00b333d46cf9 emmo:EMMO_d7788d1a_020d_4c78_85a1_13563fcec168 ) ] ;
    skos:prefLabel "Sign"@en .


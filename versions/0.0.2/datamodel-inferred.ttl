datamodel:dataModelRelation a owl:ObjectProperty ;
    rdfs:subPropertyOf emmo:EMMO_ec2472ae_cf4a_46a5_8555_1556f5a6c3c5 .

datamodel:hasRelation a owl:DatatypeProperty ;
    rdfs:comment """In the case that the type of a property refer to an entity, 'hasRelation' can be used to specify the type of relation between the property and the referred entity. 

Typical values are:
  "dm:composition"
  "dm:connection"
  "dm:relation"
or any of their sub-properties."""@en ;
    rdfs:subPropertyOf datamodel:dataRelation ;
    skos:prefLabel "hasRelation"@en .

datamodel:hasType a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:comment "May change in the future."@en,
        "Refer to a comment. Normally just a string with a type name, like \"float\". Ideally this should be an object property referring to a type in an ontology."@en ;
    rdfs:domain datamodel:Property ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf datamodel:dataComposition ;
    skos:prefLabel "hasType"@en .

datamodel:instanceOf a owl:AnnotationProperty ;
    rdfs:comment "Relates an instance to the class that describes it."@en,
        """The semantic meaning of 'instanceOf' is the same as 'rdf:type'.

But since first order logic, of which OWL descriptive logic is based on, we introduce 'instanceOf' as an annotation property."""@en ;
    rdfs:domain datamodel:Instance ;
    rdfs:isDefinedBy <rdf:type> ;
    rdfs:range datamodel:Metadata ;
    skos:prefLabel "instanceOf"@en .

datamodel:relation a owl:ObjectProperty ;
    rdfs:comment """In UML this is is a subproperty of association that is not an aggregation.

In EMMO this may be interpreted as 'semiotical'."""@en,
        "Note that this object property is not in the datamodel ontology, but may be used as a value of the 'hasRelation' data property of 'Property'."@en ;
    rdfs:domain datamodel:DataModel ;
    rdfs:range datamodel:DataModel ;
    rdfs:subPropertyOf emmo:EMMO_2337e25c_3c60_43fc_a8f9_b11a3f974291,
        owl:topObjectProperty ;
    skos:prefLabel "relation"@en .


    owl:equivalentClass [ a owl:Class ;
            owl:unionOf ( emmo:EMMO_1efe8b96_e006_4a33_bc9a_421406cbb9f0 emmo:EMMO_4f226cf3_6d02_4d35_8566_a9e641bc6ff3 ) ] ;
